#:import Label kivy.uix.label.Label
#:import ToggleButton kivy.uix.togglebutton
#:import os os

<MyWidget>:
    idfpath:idfpath
    id:my_widget
    orientation:"vertical"
    FileChooserListView:
        id:filechooser
        on_selection: my_widget.selected(filechooser.selection)
        size_hint:1, 1
        filters: ['*.idf']
    Label:
        text:"Select an IDF file"
        size_hint:1, 0.10
    ToggleButton:
        id:idfpath
        text:"Load"
        font_name:"trebuc"
        background_color:"#87b87b"
        color:(1,1,1,1)
        on_release:my_widget.save(root.idfpathlist,filechooser.path, filechooser.selection)
        size_hint:1, 0.10

<MyWidgetsavefolder>:
    savefolderpath:savefolderpath
    id:my_widget_savefolder
    orientation:"vertical"
    FileChooserIconView:
        id:filechooser
        on_selection: my_widget_savefolder.selected(filechooser.selection)
        size_hint:1, 1
        dirselect: True
        filters: ['']
    Label:
        text:"Select a Save Folder"
        size_hint:1, 0.10
    ToggleButton:
        id:savefolderpath
        text:"Load"
        font_name:"trebuc"
        background_color:"#87b87b"
        color:(1,1,1,1)
        on_release:my_widget_savefolder.savefolder(filechooser.path, filechooser.selection)
        size_hint:1, 0.10

